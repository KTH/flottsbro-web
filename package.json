    "kth-node-cache": "KTH/kth-node-cache.git#v1.0.0",
    "kth-node-configuration": "^2.0.1",
    "kth-node-cortina-block": "^3.0.5",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-i18n": "^1.0.2",
    "kth-node-ldap": "^3.0.6",
