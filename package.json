{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "flottsbro-web",
  "version": "2.0.1",
  "description": "Template project for a node express web server",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "scripts": {
    "test": "NODE_ENV=development mocha --exit test/*.js",
    "test-integration-in-docker": "ID=$(docker build -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-integration-tests.yml up --abort-on-container-exit --always-recreate-deps",
    "test-unit-in-docker": "ID=$(docker build -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-unit-tests.yml up --abort-on-container-exit --always-recreate-deps",
    "docker": "./build.sh",
    "build": "NODE_ENV=production gulp build",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development gulp build && cross-env DISPLAY_APPS_IN_CLUSTER=reference NODE_ENV=development concurrently --kill-others \"nodemon --ignore dist/ app.js\" \"gulp watch\""
  },
  "dependencies": {
